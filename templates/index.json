    "165170353504cb7d61": {
      "type": "heading",
      "blocks": {
        "1651703535bf736dff-0": {
          "type": "heading",
          "settings": {
            "heading": "Responds only to relevant alerts",
            "heading_size": "h1"
          }
        }
      },
      "block_order": [
        "1651703535bf736dff-0"
      ],
      "settings": {
        "color_scheme": "background-1",
