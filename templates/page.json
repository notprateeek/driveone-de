        "1657998406fa75ebf0-1": {
          "type": "text",
          "settings": {
            "text": "<p>Kamerataste für Radarkameras<\/p>",
            "hr": true,
            "text_style": "subtitle"
          }
        },
        "83d15a4b-f2f3-489c-b2da-b95e505d5982": {
          "type": "text",
          "settings": {
            "text": "<p>Warndreieckstaste für andere Ereignisse<\/p>",
