    "165799845181853897": {
      "type": "rich-text",
      "blocks": {
        "16579984511300d3a0-1": {
          "type": "text",
          "settings": {
            "text": "<p>Ihre Meldung wird dann an alle anderen Drive One-Benutzer weitergegeben, damit diese gewarnt werden und gefährliche Situationen vermeiden können.<\/p>"
          }
        }
      },
