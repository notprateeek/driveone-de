    "1652902788848513d1": {
      "type": "rich-text",
      "blocks": {
        "1652902788b0c48be2-0": {
          "type": "heading",
          "settings": {
            "heading": "Cookies",
            "heading_size": "h2"
          }
        },
        "32dd7eec-1ae6-4772-b6d3-b45dc70f6994": {
          "type": "text",
          "settings": {
            "text": "<p>Cookies are text files placed on your computer to collect standard internet log information and visitor behavior information. When you visit our Website, we may collect information from you automatically through cookies or similar technology.<\/p>"
          }
        },
        "15b8c4f3-1c0f-48a9-b987-daa6dce1005f": {
          "type": "text",
          "settings": {
            "text": "<p>For further information, visit allaboutcookies.org.<\/p>"
          }
        }
      },
      "block_order": [
        "1652902788b0c48be2-0",
        "32dd7eec-1ae6-4772-b6d3-b45dc70f6994",
        "15b8c4f3-1c0f-48a9-b987-daa6dce1005f"
      ],
      "settings": {
        "color_scheme": "background-1",
