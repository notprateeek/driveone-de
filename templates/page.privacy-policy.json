        "32dd7eec-1ae6-4772-b6d3-b45dc70f6994": {
          "type": "text",
          "settings": {
            "text": "<p>Cookies are text files placed on your computer to collect standard Internet log information and visitor behavior information. When you visit our Websites, we may collect information from you automatically through cookies or similar technology.<\/p>"
          }
        },
        "15b8c4f3-1c0f-48a9-b987-daa6dce1005f": {
          "type": "text",
          "settings": {
            "text": "<p>For further information, visit allaboutcookies.org.<\/p>"
