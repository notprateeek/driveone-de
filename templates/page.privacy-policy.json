          }
        },
        "3943abd1-f3c1-4d0c-b59e-09f8c19309f3": {
          "type": "text",
          "settings": {
            "text": "<p>If your information is collected, stored and processed based solely on your consent, you may at any time withdraw your consent by contacting Needit using the above contact information.<\/p>"
          }
        }
      },
