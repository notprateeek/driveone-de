    "1652937594571fccd2": {
      "type": "unstyled-list",
      "blocks": {
        "165293759480091f67-0": {
          "type": "heading",
          "settings": {
            "heading": "How do we use your personal data?",
            "heading_size": "h2"
          }
        },
        "165293759480091f67-1": {
          "type": "text",
          "settings": {
            "text": "<p>Needit collects your personal data so that we can:<\/p>"
          }
        },
        "165293759480091f67-2": {
          "type": "button",
          "settings": {
            "text": "<p>warn other users of speed control (“Speed Warning”)<\/p>",
            "button_link": "",
            "button_style_secondary": false
          }
        },
        "44581c1d-c76f-4741-a0ee-7150aceb9eee": {
          "type": "button",
          "settings": {
            "text": "<p>warn other users of traffic incidents (“Traffic Warning”)<\/p>",
            "button_link": "",
            "button_style_secondary": false
          }
        },
        "42177416-c39c-4434-8650-9c0d8d242870": {
          "type": "button",
          "settings": {
            "text": "<p>process your order and manage your account<\/p>",
            "button_link": "",
            "button_style_secondary": false
          }
        },
        "c3e53279-ffdc-42e1-a5f1-04df7f1a7109": {
          "type": "button",
          "settings": {
            "text": "<p>e-mail you with special offers on other products and services we think you might like<\/p>",
            "button_link": "",
            "button_style_secondary": false
          }
        },
        "57455448-4a87-40df-8068-0ed743d6b2b8": {
          "type": "button",
          "settings": {
            "text": "<p>e-mail you to verify your account.<\/p>",
            "button_link": "",
            "button_style_secondary": false
          }
        }
      },
