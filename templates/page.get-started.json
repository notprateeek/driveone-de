    "1651818032581093e4": {
      "type": "custom-rich-text",
      "blocks": {
        "165181803139d950a2-1": {
          "type": "text",
          "settings": {
            "text": "<p>Drive One can be the difference that makes you avoid serious accidents.<\/p>"
          }
        },
        "68115a65-488f-4be9-bd0e-879fe6ba9d18": {
          "type": "text",
          "settings": {
            "text": "<p>Drive One has earned itself a home many times already at the first avoided fine.<\/p>"
          }
        },
        "9bdeba39-3d68-4169-a5ff-291ed1e3d76c": {
          "type": "text",
          "settings": {
            "text": "<p>Drive One alerts you in a way that you can easily decode on the go.<\/p>"
          }
        },
        "60255ec6-c659-4b60-b77f-50fb03437e55": {
          "type": "text",
          "settings": {
            "text": "<p>Drive One can be set to ignore the starch boxes you pass daily.<\/p>"
          }
        },
        "9b90f1e3-fd47-4be6-8aa4-70170b6d1160": {
          "type": "text",
          "settings": {
            "text": "<p>Drive One can save you heavy traffic and long queues at road works.<\/p>"
          }
        }
      },
