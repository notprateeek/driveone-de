    "1651782997726ce620": {
      "type": "image-with-text",
      "blocks": {
        "165178299690ec3908-1": {
          "type": "text",
          "settings": {
            "text": "<p>Sobald Ihr Drive One installiert ist, müssen Sie sich keine Gedanken mehr um ihn machen - bis er Sie alarmiert.<\/p>",
            "hr": false,
            "padding-left": false,
            "text_style": "body"
          }
        },
        "bc753a4d-4ef6-4265-86c0-1b9f6b3b9c9f": {
          "type": "text",
          "settings": {
            "text": "<p>Das Telefon und das Gerät verbinden sich vollkommen automatisch, wenn Sie sich hinter das Steuer setzen.<\/p>",
            "hr": false,
            "padding-left": false,
            "text_style": "body"
          }
        },
        "04d4faa7-ff76-4487-adde-b086510df02c": {
          "type": "text",
          "settings": {
            "text": "<p>Einfacher geht es nicht.<\/p>",
            "hr": false,
            "padding-left": false,
            "text_style": "body"
          }
        }
      },
      "block_order": [
